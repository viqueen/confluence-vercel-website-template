{
  "name": "confluence-vercel-website",
  "env": [
    {
      "name": "CONFLUENCE_USERNAME",
      "description": "Confluence username",
      "required": true,
      "type": "plain"
    },
    {
      "name": "CONFLUENCE_API_TOKEN",
      "description": "Confluence API token",
      "required": true,
      "type": "secret"
    },
    {
      "name": "CONFLUENCE_SPACE_KEY",
      "description": "Confluence space key",
      "required": true,
      "type": "plain"
    },
    {
      "name": "CONFLUENCE_SITE_NAME",
      "description": "Confluence site name",
      "required": true,
      "type": "plain"
    }
  ]
}